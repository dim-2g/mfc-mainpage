$(function () {
    $('.mogila-button').on('click touchstart', function () {
        var title = $(this).parent().find('.mogila-title').text();
        var category = $(this).parent().parent().parent().prev().text();
        $('.form-vac').fadeIn(300);
        $('html').addClass('ovHid');
        $('html').addClass('fancybox-lock');
        $('html').addClass('fancybox-margin');
        var htmlWidth = $('html').width();
        $('.main-menu').css({'width':htmlWidth});
        $('.fixed-header').css({'width':htmlWidth});
        $('.form-vacInner-name').text(title);
        $('.form-mogila .form-vacInner-title').text(category);
        $('.vacInner-kladbishe').val($(this).parent().find('.mogila-price').attr("alt"));
        $('.vacInner-price').val($(this).parent().find('.mogila-price').text());

    });
    $('.form-vacSubmitBtn').click(function(){
        $(this).parents('form').eq(0).trigger('submit');
        //$('form.form-vacForm').trigger("submit");
    });
    $('.form-mogili .form-vacForm').submit(function(e) {
        e.preventDefault();
        var f = $(this);
        $('input[type=text]', f).removeClass('error');
        $('.form-vac').find('.error.text').remove();
        var name = $('.form-vacForm input[name=name]').val();
        var phone = $('.form-vacForm input[name=phone]').val();
        var email = $('.form-vacForm input[name=email]').val();
        var type = $('.form-vacInner-name').text();
        type=$.trim(type);
        var price = $('.form-vacForm input[name=price]').val();
        price=$.trim(price);
        var kladbishe = $('.form-vacForm input[name=kladbishe]').val();
        var recaptcha = $('.form-vacForm textarea[name=g-recaptcha-response]').val();
        var error_s = false;
        if(phone=="") error_s=true;
        if(error_s) {
            $('.form-vacForm input[name=phone]').addClass('error');
            $('.form-vacForm input[name=phone]').focus();
        }
        else {
            var query = 'act=sender';
            query += '&name=' + name;
            query += '&phone=' + phone;
            query += '&email=' + email;
            query += '&type=' + type;
            query += '&price=' + price;
            query += '&kladbishe=' + kladbishe;
            query += '&g-recaptcha-response=' + recaptcha;

            $.ajax({
                type: "POST",
                data: query,
                url: "/local/functions/send_klad.php",
                dataType: "json",
                success: function(data) {
                    if(data.result == 'ok') {
                        $('.close-vac').trigger("click");
                        $("#thanks_request").show();
                    } else {
                        $('.form-vacForm').before('<div class="error text">'+data.text+'</div>');
                    }
                }
            });
        }
        grecaptcha.reset();
        return false;
    });
    $('.form-calc .form-vacForm').on("submit", function (e) {
        e.preventDefault();
        var f = $(this);
		var _active = $('.active-wrap');
        var allSummInput = document.createElement('input');
        allSummInput.setAttribute('type', 'hidden');
        allSummInput.setAttribute('name', 'allSumm');
        allSummInput.value = $('.active-wrap .allSumm').text();
		$(".hidden-form",_active).append(allSummInput);
        //document.querySelector('.hidden-form').appendChild(allSummInput);

        var buttontext = document.createElement('input');
        buttontext.setAttribute('type', 'hidden');
        buttontext.setAttribute('name', 'usluga');
        buttontext.value = $('.buttons .calc-btn.active span').text();
		$(".hidden-form",_active).append(buttontext);
        //document.querySelector('.hidden-form').appendChild(buttontext);

        $('input[type=text]', f).removeClass('ierror');
        $('.form-vac',_active).find('.error.text').remove();
        var name = $('.form-vacForm input[name=name]',_active).val();
        var phone = $('.form-vacForm input[name=phone]',_active).val();
        var agree = $('.form-vacForm input[name=agree]',_active).is(':checked');
        var error_s = false;
        if (name == "") {
            $('.form-vacForm input[name=name]',_active).addClass('ierror');
			error_s = true;
        } 
		if (phone == "") {
            $('.form-vacForm input[name=phone]',_active).addClass('ierror');
			error_s = true;
        }
		if (!agree) {
            $('.form-vacForm input[name=phone]',_active).addClass('ierror');
			error_s = true;
        }
        if(!error_s) {
            var query = $(this).serialize();
            $.ajax({
                type: "POST",
                data: query,
                url: "/local/functions/send_rit.php",
                dataType: "json",
                success: function (data) {
                    if (data.result == 'ok') {
                        $('.close-vac',_active).trigger("click");
                        $("#thanks_request").show();
                    } else {
                        $('.form-vacForm',_active).before('<div class="error text">' + data.text + '</div>');
                    }
                }
            });
        }
        grecaptcha.reset();
    });

});


