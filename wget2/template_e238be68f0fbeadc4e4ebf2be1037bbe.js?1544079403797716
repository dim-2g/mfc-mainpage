$(function(){
    $("[name=form_text_5]").suggestions({
        token: "04b7035b3043861edfcfcb5dc42fe9a799afe9cf",
        type: "NAME",
        count: 5,
        minChars: 2,
        autoSelectFirst: false,		
        onSelect: function(suggestion) {

        }
    });	
    $("[name=form_text_7]").suggestions({
        token: "04b7035b3043861edfcfcb5dc42fe9a799afe9cf",
        type: "ADDRESS",
        count: 5,
        minChars: 2,
        autoSelectFirst: false,
        onSelect: function(suggestion) {
			if(true || $.trim(suggestion.data.street) != ''){				
				$("[name=form_text_10]").val(suggestion.data.geo_lat);
				$("[name=form_text_11]").val(suggestion.data.geo_lon);
			} else {
				$("[name=form_text_10]").val('');
				$("[name=form_text_11]").val('');
			}
        },
		onSelectNothing: function(){
			$("[name=form_text_10]").val('');
			$("[name=form_text_11]").val('');
		},
		onInvalidateSelection: function(){
			$("[name=form_text_10]").val('');
			$("[name=form_text_11]").val('');
		}
    });	
})
