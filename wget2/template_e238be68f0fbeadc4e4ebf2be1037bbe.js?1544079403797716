$(function(){
	
	//     
	$(".jsCalcPd__input_multi").each(function(){
		var _this = $(this);
		_this.clone().addClass(_this.attr('data-class-pattern')).insertBefore(_this);
	});
	
	//       
    $(document).click(function (e) {
        if (!$(e.target).hasClass('jsCalcPd__select-option')) {
			var inputValue = $('.jsCalcPd__input-value').eq(0);			
            $('.jsCalcPd__select-value').css('display','none');
			$('.jsCalcPd__input-value').removeClass(inputValue.attr('data-class-open'));
        }
    });
	
	//\  
	$(document).on('click','.jsCalcPd__select',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var inputValue = _this.parents('.jsCalcPd__input-value').eq(0);
		var selectValue = $('.jsCalcPd__select-value',inputValue).eq(0);
		
		$('.jsCalcPd__select-value').not(selectValue).css('display','none');
		$('.jsCalcPd__input-value').not(inputValue).removeClass(inputValue.attr('data-class-open'));
		selectValue.slideToggle();
		inputValue.toggleClass(inputValue.attr('data-class-open'));
		
		return false;
	});
	
	//    
	$(document).on('click','.jsCalcPd__select-option',function(e){
		e.preventDefault();
		var _this = $(this);
		
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var input = _this.parents('.jsCalcPd__input').eq(0);
		var inputValue = _this.parents('.jsCalcPd__input-value').eq(0);
		var select = $('.jsCalcPd__select',inputValue).eq(0);
		var selectText = $('.jsCalcPd__select-text',_this).text();

		var disableIfSelect;
		try {
			disableIfSelect = JSON.parse(input.attr('data-disable-if-select'));
		} catch(e){
			disableIfSelect = false;
		}
		
		var hideIfSelect;
		try {
			hideIfSelect = JSON.parse(input.attr('data-hide-if-select'));
		} catch(e){
			hideIfSelect = false;
		}
		
		var showIfSelect;
		try {
			showIfSelect = JSON.parse(input.attr('data-show-if-select'));
		} catch(e){
			showIfSelect = false;
		}
		console.log(showIfSelect);
		$('.jsCalcPd__select-value').css('display','none');
		$('.jsCalcPd__input-value').removeClass(inputValue.attr('data-class-open'));
		
		var price = _this.attr('data-price') || 0;
		var value = _this.attr('data-value') || 0;
		
 		if(disableIfSelect.length){
			if(value != 0) {
				for(var i=0;i<disableIfSelect.length;i++){
					field = $("[data-name='info["+disableIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.addClass(section.attr('data-class-disabled'));
					$(".jsCalcPd__select",section).attr('data-price','0');
					$(".jsCalcPd__select",section).text(' ');
					$(".jsCalcPd__price",section).attr('data-price','0');
					$(".jsCalcPd__price",section).text('0');
					$('.jsCalcPd__select',section).removeClass($('.jsCalcPd__select',section).eq(0).attr('data-class-error'));
					$('.jsCalcPd__error',section).removeClass($('.jsCalcPd__error',section).eq(0).attr('data-class-active'));
				}
			} else {
				for(var i=0;i<disableIfSelect.length;i++){
					field = $("[data-name='info["+disableIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.removeClass(section.attr('data-class-disabled'));				
				}
			}
		}
		
 		if(hideIfSelect.length){
			if(value != 0) {
				for(var i=0;i<hideIfSelect.length;i++){
					field = $("[data-name='info["+hideIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.addClass(section.attr('data-class-hidden'));
					$(".jsCalcPd__select",section).attr('data-price','0');
					$(".jsCalcPd__select",section).text(' ');
					$(".jsCalcPd__price",section).attr('data-price','0');
					$(".jsCalcPd__price",section).text('0');
					$('.jsCalcPd__select',section).removeClass($('.jsCalcPd__select',section).eq(0).attr('data-class-error'));
					$('.jsCalcPd__error',section).removeClass($('.jsCalcPd__error',section).eq(0).attr('data-class-active'));
				}
			} else {
				for(var i=0;i<hideIfSelect.length;i++){
					field = $("[data-name='info["+hideIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.removeClass(section.attr('data-class-hidden'));				
				}
			}
		}

 		if(showIfSelect.length){
			if(value == 0) {
				for(var i=0;i<showIfSelect.length;i++){
					field = $("[data-name='info["+showIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.addClass(section.attr('data-class-hidden'));
					console.log(section);
					$(".jsCalcPd__select",section).attr('data-price','0');
					$(".jsCalcPd__select",section).text(' ');
					$(".jsCalcPd__price",section).attr('data-price','0');
					$(".jsCalcPd__price",section).text('0');
					$('.jsCalcPd__select',section).removeClass($('.jsCalcPd__select',section).eq(0).attr('data-class-error'));
					$('.jsCalcPd__error',section).removeClass($('.jsCalcPd__error',section).eq(0).attr('data-class-active'));
				}
			} else {
				for(var i=0;i<showIfSelect.length;i++){
					field = $("[data-name='info["+showIfSelect[i]+"][]']",parent);
					var section = field.parents('.jsCalcPd__section').eq(0);
					section.removeClass(section.attr('data-class-hidden'));				
				}
			}
		}
		
		select.removeClass(select.attr('data-class-error'));
		$(".jsCalcPd__error",inputValue).removeClass($(".jsCalcPd__error",inputValue).eq(0).attr('data-class-active'));
		select.text(selectText);
		select.attr('data-price',price);
		select.attr('data-value',value);
		
		var sum = jsCalcPdGetInputSum(input);
		$(".jsCalcPd__price",input).attr('data-price',sum).text(priceFormat(sum));
		if(sum > 0){
			input.addClass(input.attr('data-class-active'));
			inputValue.addClass(inputValue.attr('data-class-active'));
		} else {
			input.removeClass(input.attr('data-class-active'));
			inputValue.removeClass(inputValue.attr('data-class-active'));
		}

		var total = jsCalcPdGetTotalSum(parent);
		$(".jsCalcPd__total",parent).attr('data-price',total).text(priceFormat(total));	
		
		var desc = $("[data-id="+value+"]",inputValue);
		$(".jsCalcPd__detail-option",inputValue).each(function(){
			var option = $(this);
			option.removeClass(option.attr('data-class-active'));
		});
		if(desc.length){
			desc.addClass(desc.attr('data-class-active'));
		}
		//  
		if(input.hasClass('jsCalcPd__input_toggle')){
			$(".jsCalcPd").addClass($(".jsCalcPd").eq(0).attr('data-class-hidden'));
			$(".jsCalcPd[data-id="+value+"]").removeClass($(".jsCalcPd").eq(0).attr('data-class-hidden'));
			$(".jsCalcPd__input_toggle .jsCalcPd__select").text(selectText);
			$(".jsCalcPd__input_toggle .jsCalcPd__select").attr('data-price',price).attr('data-value',value);
		}
		return false;
	});

	//   
	$(document).on('click','.jsCalcPd__btn-add',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var input = _this.parents('.jsCalcPd__input').eq(0);
		var inputValue = _this.parents('.jsCalcPd__input-value').eq(0);
		var newValue = inputValue.clone()
		
		$(".jsCalcPd__select",newValue).text(' ');
		newValue.removeClass(newValue.attr('data-class-pattern'));
		newValue.insertAfter(inputValue);
		
		jsCalcPdButtonManage(input);
		return false;
	});	
	
	//   
	$(document).on('click','.jsCalcPd__btn-remove',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var input = _this.parents('.jsCalcPd__input').eq(0);
		var inputValue = _this.parents('.jsCalcPd__input-value').eq(0);
		inputValue.remove();
		jsCalcPdButtonManage(input);
		
		var sum = jsCalcPdGetInputSum(input);
		$(".jsCalcPd__price",input).attr('data-price',sum).text(priceFormat(sum));		
		
		var total = jsCalcPdGetTotalSum(parent);
		$(".jsCalcPd__total",parent).attr('data-price',total).text(priceFormat(total));		
		
		return false;
	});	
	
	
	//,   
	$(document).on('click','.jsCalcPd__order',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var hasError = false;
		$(".jsCalcPd__input_required .jsCalcPd__price",parent).each(function(){
			var __this = $(this);
			var _parent = __this.parents('.jsCalcPd__input').eq(0);
			var _section = __this.parents('.jsCalcPd__section').eq(0);
			if(
				!(
					_section.hasClass(_section.attr('data-class-disabled'))
					||_section.hasClass(_section.attr('data-class-hidden'))
				)
			){
				if(!_parent.hasClass(_parent.attr('data-class-pattern'))) {			
					var price = parseInt(__this.attr('data-price')) || 0;
					if(price == 0){
						if(!hasError){
							hasError = $('.jsCalcPd__select',_parent).eq(0);
						}
						$('.jsCalcPd__select',_parent).addClass($('.jsCalcPd__select',_parent).eq(0).attr('data-class-error'));
						$('.jsCalcPd__error',_parent).addClass($('.jsCalcPd__error',_parent).eq(0).attr('data-class-active'));
					} else{
						$('.jsCalcPd__select',_parent).removeClass($('.jsCalcPd__select',_parent).eq(0).attr('data-class-error'));				
						$('.jsCalcPd__error',_parent).removeClass($('.jsCalcPd__error',_parent).eq(0).attr('data-class-active'));
					}
				}
			}
		});
		
		if(!hasError) {

			var form = $(".jsCalcPd__form",parent);
			var canSubmit = true;
			$(".jsField",form).each(function(){

				if(!jsFieldValide($(this))){
					canSubmit = false;
				}
			});
			if(canSubmit){
				var post = form.serializeArray();
				post.push({
					name: 'sessid',
					value: SESSID,
				});
				$(".jsCalcPd__select",parent).each(function(){
					var input = $(this).parents('.jsCalcPd__input').eq(0);
					if(!input.hasClass(input.attr('data-class-pattern'))){
						post.push({
							name: $(this).attr('data-name'),
							value: $(this).attr('data-value'),
						})
					}
				});
				parent.addClass(parent.attr('data-class-loading'));
				$.post(form.attr('action'),post,function(data){
					
					if(data.status == true){
						$.fancybox.close();
						alert(data.message.join());
						location.reload();
					} else {
						alert(data.message.join());
						parent.removeClass(parent.attr('data-class-loading'));						
					}
				},'json');
			}
		} else {
            $('body, html').animate({
                scrollTop: hasError.offset().top - 256
            }, 500);
		}
		return false;
	});
	
	$(document).on('submit','.jsCalcPd__form',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var popup = $(".jsCalcPd__popup",parent);
		var canSubmit = true;
		$(".jsField",popup).each(function(){
			if(!jsFieldValide($(this))){
				canSubmit = false;
			}
		});
		
		if(canSubmit){
			var post = _this.serializeArray();
			post.push({
				name: 'sessid',
				value: SESSID,
			});
			$(".jsCalcPd__select",parent).each(function(){
				var input = $(this).parents('.jsCalcPd__input').eq(0);
				if(!input.hasClass(input.attr('data-class-pattern'))){
					post.push({
						name: $(this).attr('data-name'),
						value: $(this).attr('data-value'),
					})
				}
			});
			
			$.post(_this.attr('action'),post,function(data){
				if(data.status == true){
					$.fancybox.close();
					alert(data.message.join());
					location.reload();
				} else {
					alert(data.message.join());
				}
			},'json');
		}
		return false;
	});
	
	$(".jsCalcPd__input").each(function(){
		jsCalcPdButtonManage($(this));
	});	
});

function jsCalcPdButtonManage(input) {
	$(".jsCalcPd__btn-add",input).css('display','none');
	$(".jsCalcPd__btn-remove",input).css('display','inline-block');
	
	$(".jsCalcPd__btn-remove",input).eq(0).css('display','none');	
	$(".jsCalcPd__btn-add:last",input).css('display','inline-block');
}

function jsCalcPdGetInputSum(input) {
	var sum = 0;
	$(".jsCalcPd__select",input).each(function(){
		sum += parseInt($(this).attr('data-price')) || 0;
	})
	return sum;
}

function jsCalcPdGetTotalSum(calc) {
	var sum = 0;
	$(".jsCalcPd__price",calc).each(function(){
		sum += parseInt($(this).attr('data-price')) || 0;
	})
	return sum;
}

/* End */
;
; /* Start:"a:4:{s:4:"full";s:49:"/local/templates/ritual/js/form.js?15409748155743";s:6:"source";s:34:"/local/templates/ritual/js/form.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function(){
	$(document).on('submit','.jsForm__form',function(e){
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var form = $(".jsForm__form",parent);
		
		var stepActiveClass = parent.attr('data-step-active-class');
		
		if(parent.hasClass('jsForm_ajax')){
			e.preventDefault();
			var canSubmit = true;
			var currentStep = parent
			if(parent.hasClass('jsForm_steps')){
				currentStep = $("."+stepActiveClass,parent);
			}
			$(".jsField",currentStep).each(function(){
				if(!jsFieldValide($(this))){
					canSubmit = false;
				}
			});
			var formField;
			$(".jsForm__field_required",currentStep).each(function(){
				formField = $(this);
				if($.trim(formField.val()).length == ''){
					if(canSubmit){
						alert(formField.attr('data-error-message'));
						canSubmit = false;
					}
				}
			});
			
			if(canSubmit){
				if(parent.hasClass('jsForm_steps')){
					var nextStep = currentStep.next();
					if(parent.hasClass('jsForm_online')){
						if(currentStep.index() == 0){
							$(".jsForm__phone-label",parent).text($('.jsForm__phone',parent).val());
							parent.addClass(parent.attr('data-class-loading'));
							$.post(
								'/api/confirm-phone.php',
								{
									'sessid':SESSID,
									'number':$('.jsForm__phone',parent).val()
								},function(data){
									parent.removeClass(parent.attr('data-class-loading'));
									if(data.status) {
										jsFormStep(parent,'next');
										return false;
									} else {
										if(typeof $.fancybox.open == 'function'){
											$.fancybox.open(data.message.join('<br>'));
										} else {
											$.fancybox("<div class='form__message'>"+data.message.join('<br>')+"</div>");
										}
										return false;
									}
								},
								'json'
							).fail(function(data){
								parent.removeClass(parent.attr('data-class-loading'));
								if(typeof $.fancybox.open == 'function'){
									$.fancybox.open("      .       <a class='roistat-phone' href='tel:+74951003100'>8 (495) 100-3-100</a>   ");									
								} else {
									$.fancybox("<div class='form__message'>      .       <a class='roistat-phone' href='tel:+74951003100'>8 (495) 100-3-100</a>   </div>");
								}
								jsFormStep(parent,'prev');
								return false
							});
							return false;
						}
					} else {
						if(nextStep.length == 1){
							$(".jsForm_step",parent).removeClass(stepActiveClass);
							currentStep.next().addClass(stepActiveClass);
							return false;
						}
					}
				}
				var requestData = new FormData();
				
				// 
				$.each($("input[type='file']",form), function(i, tag) {
					$.each($(tag)[0].files, function(i, file) {
						requestData.append(tag.name,file);
					});
				});
				// 
				requestData.append('sessid',SESSID);
				
				// 
				var params = form.serializeArray();
				$.each(params, function (i, val) {
					requestData.append(val.name, val.value);
				});				
				parent.addClass(parent.attr('data-class-loading'));
				$.ajax({
					url: _this.attr('action'),
					data: requestData,
					cache: false,
					contentType: false,
					processData: false,
					method: 'POST',
					dataType: 'json',
					success: function(data){
						parent.removeClass(parent.attr('data-class-loading'));
						if(data.status == true){
							form[0].reset();
							
							if(parent.hasClass('jsForm_online')){
								$.fancybox.close();
								jsFormStep(parent,'prev');
							}
							
							if(parent.attr('data-need-close') == "Y"){
								$.fancybox.close();
							}
							
							if(typeof $.fancybox.open == 'function'){
								$.fancybox.open(data.message.join('<br>'));
							} else {
								$.fancybox("<div class='form__result'>"+data.message.join('<br>')+"</div>");
							}
							if(parent.attr('data-need-reload') == "Y"){
								location.reload();
							} else {
								form[0].reset();
							}
						} else {
							if(typeof $.fancybox.open == 'function'){
								$.fancybox.open(data.message.join('<br>'));
							} else {
								$.fancybox("<div class='msg'><div class='msg__text'>"+data.message.join('<br>')+"</div></div>");
							}
						}
					},
					error: function(data){
						parent.removeClass(parent.attr('data-class-loading'));
						if(typeof $.fancybox.open == 'function'){
							$.fancybox.open('');
						} else {
							$.fancybox('');
						}
					}
				});
			}
			return false;	
		}
	});
	

	$(document).on('click','.jsForm__prev-step',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
			jsFormStep(parent,'prev');
		return false;
	});	
})

function jsFormStep(jsForm,step){
	var stepActiveClass = jsForm.attr('data-step-active-class');	
	var currentStep = $("."+stepActiveClass,jsForm);	
	if(step == 'prev'){
		var prevStep = currentStep.prev();
		if(prevStep.length == 1){
			$(".jsForm_step",jsForm).removeClass(stepActiveClass);
			prevStep.addClass(stepActiveClass);
		}
	} else if(step == 'next') {
		var nextStep = currentStep.next();
		if(nextStep.length == 1){
			$(".jsForm_step",jsForm).removeClass(stepActiveClass);
			nextStep.addClass(stepActiveClass);
		}
	}
}
/* End */
;
; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/ritual/js/field.js?15409748151609";s:6:"source";s:35:"/local/templates/ritual/js/field.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function(){
	
	$(document).on('change','.jsField__required',function(e){
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		jsFieldValide(parent);
	});
	$(document).on('keyup','.jsField__required',function(e){
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		jsFieldValide(parent);
	});
	
})

function jsFieldValide(field){
	var input = $(".jsField__required",field);
	var inputError = $(".jsField__error",field);
	if(input.length > 0){
		if(input.attr('type') == 'text'){
			var val = $.trim(input.val());
			if(val.length == 0 ){
				input.addClass(input.attr('data-class-error'));
				inputError.addClass(inputError.attr('data-class-active'));
				return false;
			} else {
				input.removeClass(input.attr('data-class-error'));
				inputError.removeClass(inputError.attr('data-class-active'));				
				return true;
			}
		} else if(input.attr('type') == 'checkbox'){
			if(input.is(':checked')){
				inputError.removeClass(inputError.attr('data-class-active'));				
				return true;
			} else {
				inputError.addClass(inputError.attr('data-class-active'));				
				return false;
			}
		}else if(input.get(0).tagName == 'SELECT'){
			var val = $.trim(input.val());
			if(val.length == 0 ){
				input.addClass(input.attr('data-class-error'));
				inputError.addClass(inputError.attr('data-class-active'));
				return false;
			} else {
				input.removeClass(input.attr('data-class-error'));
				inputError.removeClass(inputError.attr('data-class-active'));				
				return true;
			}
		}
		
	}
	return true;
}
/* End */
