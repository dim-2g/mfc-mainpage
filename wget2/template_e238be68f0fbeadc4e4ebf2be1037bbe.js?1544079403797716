$(function(){

    $(document).on("click", ".js-tabel-form-show", function(e){
        e.preventDefault();
        var $form = $(this).closest(".js-tabel-form");
        $form.find(".js-tabel-form-message").removeClass("active");
        $form[0].reset();
    });
});



/*

LANDING PAGE //////

