function bind_widgets() {

  $('.mask-phone-ru').not(".mask-inited").inputmask({
      definitions: {
          '#': {
              validator: "[0-9]",
              cardinality: 1
          }
      },
      mask: "+7 (###) ### ## ##",
      placeholder: "_"
  }).addClass("mask-inited");

  $('.mask-tabel-number').not(".mask-inited").inputmask('999').addClass("mask-inited");
  img_to_bg();
  checkbox_plain_init();
}


/*

Set Background Image depending on img content inside it

 */

function img_to_bg($o) {
  if (typeof $o === "undefined") $o = $(".img-to-bg");
  $o.each(function () {
    if ($(this).find("> img").length && $(this).find("> img").attr("src")) {
      $(this).css("background-image", "url('" + $(this).find("> img").attr("src") + "')");
      $(this).addClass("img-to-bg--inited");
    }
  });
}


/*

Custom Checkboxes

 */

function checkbox_plain_init() {
  $(".checkbox-plain-js").find("[type='checkbox'], [type='radio']").not(".checkbox-plain-js-inited").on("change change-pseudo", function(e){
    var $o = $(this).closest(".checkbox-plain-js");
    if ($(this).is(":radio"))
    {
      var name = $(this).attr("name");
      var $radios = $(this).closest("form").find("[name='"+name+"']");
      if (!$radios.length) $radios = $("input[name='"+name+"']");
      $radios.closest(".checkbox-plain-js").removeClass("checked");
    }
    if ($(this).prop("checked")) $o.addClass("checked");
    else $o.removeClass("checked");
    if ($(this).prop("disabled")) $o.addClass("disabled");
    else $o.removeClass("disabled");
  }).each(function(){
    var $o = $(this).closest(".checkbox-plain-js");
    if ($(this).prop("checked")) $o.addClass("checked");
    if ($(this).prop("disabled")) $o.addClass("disabled");
  }).addClass("checkbox-plain-js-inited").closest(".checkbox-plain-js").addClass("checkbox-plain-js-inited").find("input:text").on("focus", function(){
    $(this).closest(".checkbox-plain-js").find(":checkbox,:radio").prop("checked", true).trigger("change");
  });
}


function form_validate_init() {
  $(document).on("reset", ".form-validate", function(){
    $(this).find(".form__item, :input").removeClass("error");
  }).on("submit", ".form-validate", function(e){
    $this = $(this);
    error = check_form($(this), true);
    if (!error)
    {
      if ($(this).attr("data-action"))
      {
        e.preventDefault();
        var data = $(this).serialize();

        if (window.FormData !== undefined) {
          data = new FormData($(this)[0]);
        }

        $.ajax({
          url: $(this).attr("data-action"),
          type: $(this).attr("method"),
          data: data,
          dataType: 'json',
          cache: false,
          contentType: false,
          processData: false,
          beforeSend: function() {
            $this.find(":input").prop("disabled", true);
          },
          complete: function(response) {
            $this.find(":input").prop("disabled", false);
          },
          success: function(response) {
            if (response.result)
            {
              if (response.redirect) location.href = response.redirect;
              $this[0].reset();
              $this.addClass("form-send");
              $($this.data("message")).find(".message__title, .js-message-title").html(response.message);
              $($this.data("message")).find(".message__summary, .js-message-summary").html(response.message_text);
              $($this.data("message")).addClass("active");
              if ($this.data("autoclose-message"))
              {
                setTimeout(function(){
                  $($this.data("message")).removeClass("active");
                  $this.addClass("active");
                  $this.removeClass("form-send");
                }, $this.data("autoclose-message"));
              }
            }
            else
            {
              $field = $this.find("[name='"+response.name+"']");
              $field.addClass("error");
              $field.parent().append("<div class='form__error'>"+response.message_text+"</div>");
            }
          },
          error: function(jqXHR, textStatus, errorMessage) {
            console.log(errorMessage);
          }
        });
      }
    }
    else {
      e.preventDefault();
    }
  });
}

function check_form($form, highlight, handler)
{
  if (typeof highlight === "undefined") highlight = true;
  var error = false;
  if (highlight)
  {
    $form.find(":input").removeClass("error").addClass("success").closest(".form__item-field").closest(".form__item").removeClass("error").addClass("success");
    $form.find(".form__error").remove();
  }
  $form.find(":input.required").each(function(){
    if ($(this).closest(".expand-it-container").not(".active").length) return true;
    if (!$(this).val())
    {
      error = true;
      if (highlight)
      {
        $(this).addClass("error");
      }
      if (highlight)
      {
        $(this).closest(".form__item").addClass("error");
        if (!$(this).is("select")) $(this).closest(".form__item-field").append("<div class='form__error'> </div>");
        else $(this).closest(".form__item-field").append("<div class='form__error'>"+(($(this).attr("data-error-message"))?$(this).attr("data-error-message"):"     .")+"</div>");
      }
    }
  });
  $form.find(":input").each(function(){
    if ($(this).closest(".expand-it-container").not(".active").length) return true;
    if ($(this).val())
    {
      $email = $(this).filter(":input.email-validate");
      if ($email.length && !validateEmail($email.val()))
      {
        error = true;
        if (highlight) $email.addClass("error");
        if (highlight) $email.closest(".form__item-field").append("<div class='form__error'>  E-mail</div>");
      }
      $phone = $(this).filter(":input.phone-validate");
      if ($phone.length && $phone.val() && $phone.val().replace(/[\D]/g, "").length < 11)
      {
        error = true;
        if (highlight) $phone.addClass("error");
        if (highlight) $phone.closest(".form__item-field").append("<div class='form__error'>  </div>");
      }
    }
  });

  $form.find(".checkboxes.required").each(function(){
    if ($(this).closest(".expand-it-container").not(".active").length) return true;
    var checked = 0;
    var radio = 0;
    $(this).find(":radio, :checkbox").each(function(){
      if ($(this).prop("checked"))
      {
        checked++;
      }
      if ($(this).attr("type") == "radio")
      {
        radio++;
      }
    });
    if (!checked)
    {
      error = true;
      if (highlight)
      {
        $(this).closest(".form__item-field").append("<div class='form__error'>"+((radio)?"   .":"    .")+"</div>");
      }
    }
  });

  $form.find(":radio, :checkbox").filter(".required").each(function(){
    if ($(this).closest(".expand-it-container").not(".active").length) return true;
    if (!$(this).prop("checked"))
    {
      error = true;
      if (highlight)
      {
        $(this).closest(".form__item-field").append("<div class='form__error'>"+(($(this).attr("data-error-message"))?$(this).attr("data-error-message"):"    .")+"</div>");
        $(this).addClass("error");
      }
    }
  });

  $form.find(":input.error").removeClass("success");
  $form.find(".form__item.error").removeClass("success");

  if (highlight)
  {
    $form.find(":input.error:first").focus();
  }

  if (typeof handler !== "undefined") error = handler($form, highlight) || error;

  $form.find(".form__error").addClass("active");

  return error;
}

function validateEmail(email) {
  var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
  return re.test(email);
}

$(function(){

  bind_widgets();
  $(document).ajaxStop(function () {
    bind_widgets();
  });

  expand_it_init();
  toggle_element_init();
  tabs_init();


    $(".js-close-pregions").on("click", function (e) {
        e.preventDefault();
        $(this).closest(".pregions").removeClass("active");
    });

    $(".js-open-pregions").on("click", function (e) {
        e.preventDefault();
        $(".pregions").addClass("active");
    });

  $(document).on('click', '.goto', function(e){
    var $o = $($(this).attr("href"));
    if (!$o.length) {
      var $o = $($(this).data("href"));
    }
    if ($o.length)
    {
      e.preventDefault();
      $("html,body").animate({
        scrollTop: $o.offset().top + ($(this).data("goto-offset")?$(this).data("goto-offset"):0) - ($(this).data("goto-offset-header")?$(".top-red-banner").outerHeight() + $(".header").outerHeight():0)
      }, $(this).data("goto-speed")?$(this).data("goto-speed"):500);
    }
  });

  $(document).on("click", ".video-block:not(.active)", function(e){
    e.preventDefault();
    $(this).addClass("active");
    var $v = $(this).find(".video-block__video");
    var $vb = $v.closest(".video-block");
    var $vi = $(this).find(".video-block__video-inner");
    $vi.html('<iframe width="1280" height="720" src="'+$v.data("link")+'" frameborder="0" allowfullscreen></iframe>');
    var id = $vb.attr("id");
    if (!id)
    {
      id = "id"+(new Date()).getTime();
      $vb.attr("id", id);
    }
    addCSSRule("rule-"+id, "#"+id+".active { height: "+ ($vb.find("iframe").height() + ($vb.outerHeight() - $vb.closest(".video-block").height()))+"px; }");
  });

  $(window).on("load", function(){

    if ($('.l-slider').length) {

      $('.l-slider').slick({
        infinite: true,
        slidesToShow: 1,
        slidesToScroll: 1,
        speed: 500,
        arrows: false
      });

      $('.l-slider__prev').click(function () {
        $('.l-slider').slick('slickPrev');
      });

      $('.l-slider__next').click(function () {
        $('.l-slider').slick('slickNext');
      });

    }

    $(".smi").each(function(){
        var $slide = $("<div class='smi-slider__item'>");
        $slide.append($(this).children().clone());
        $('.smi-slider').append($slide);
    });

      $('.smi-slider').owlCarousel({
          loop: true,
          nav: false,
          dots: false,
          center: true,
          items: 3
      });


      $('<div class="lnews-slider-wrapper">\
            <div class="lnews-slider owl-carousel">\
            </div>\
        </div>').insertAfter('.lnews-list');

      $(".lnews").each(function(){
          var $slide = $("<div class='lnews-slider__item'>");
          $slide.append($(this).children().clone());
          $('.lnews-slider').append($slide);
      });

      $('.lnews-slider').owlCarousel({
          loop: true,
          nav: false,
          dots: false,
          center: true,
          items: 3
      });

    if (document.getElementById('l-form-recaptcha')) {
      grecaptcha.render('l-form-recaptcha', {
        'sitekey' : '6LcouRkTAAAAAOJVhjoSEV9SHARazZwwjUvdN4Tk'
      });
    }

  });

});



