$(function(){
	var mainBanner = $('.jsMainBanner');
	if (mainBanner.length) {
		var mainBannerPopup = $('.jsMainBanner__popup',mainBanner);
		$(window).scroll(function(){
			var scroll = $(document).scrollTop();
			if (scroll > 128) {
				mainBannerPopup.addClass(mainBannerPopup.attr('data-class-unactive'));
			} else {
				mainBannerPopup.removeClass(mainBannerPopup.attr('data-class-unactive'));
			}
		})
	}
