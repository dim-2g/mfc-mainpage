$(function(){

	if(window.location.href.indexOf("ritualnoe-strakhovanie/#") > -1){ //looking for hashtags in the address
		var address = window.location.href;
		var parts = address.split("#");
		var anchor = '#' + parts[1];
		var offset = $(anchor).offset().top;
		if($('body').outerWidth(true) > 1030){
			$('html, body').animate({
				scrollTop: offset - 250,
			}, 300);
		} else{
			$('html, body').animate({
				scrollTop: offset - 90,
			}, 300);
		}
	}

});

