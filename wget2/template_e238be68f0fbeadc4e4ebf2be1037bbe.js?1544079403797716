$(function () {
  /**
   * Go To Node
   */
  $.fn.SlideToNode = function(speed, offset_y) {
    if (!offset_y)
      offset_y = 0;
    $('body,html').animate({
      scrollTop: $(this).offset().top + offset_y
    }, speed?speed:0);
  };

    $('.main-bottom-img.main-bottom-rit').click(function(){
      location.href = '/about/gbu-ritual/';
    });
    $('.search-page .search-submit').attr('value', '');
    if($(window).width() < 600){
        $('.search-page .search-submit').attr('value', '');
    }
    $('.mobileVert-img.mobileVert-rgs').on('click', function(){
        location.href = '/poleznaya-informacia/articles/gost-na-ritualnye-uslugi/';
    });
    $('.main-bottom-img.main-bottom-my-docs').on('click', function(){
        location.href = '/ritualnie-uslugi/ritualnoe-strakhovanie/';
    });

    if(!$('.rit-left').length) {
        $('.top-red-banner').css({
            transition: 'none'
        }).addClass('deployed');
        $('.header.inner-header').css({
            transition: 'none'
        }).addClass('deployed');
        $('.main-menu.main-menu_test').addClass('deployed');
    }
    $('.coffin-list-item').click(function(){
        $(this).parent().slideUp(300);
        $(this).parent().parent().removeClass('deployed');

    })
    $(".respTextArea").keyup(function () {
        if (this.value.length > 5000)
            this.value = this.value.substr(0, 5000);
    });


    $('.zapros-form').scroll(function (e) {
        var offsetLeft;

        if ($(window).height() >= 700) {
            offsetLeft = $('.kalendar').offset().top - $(window).height() / 2 - $('.form-mogila').height() / 2 + 320;
            $('#ui-datepicker-div').css({'top': offsetLeft});
        }
        else if ($(window).height() <= 700) {
            offsetLeft = $('.kalendar').offset().top - $(window).height() / 2 - $('.form-mogila').height() / 2 - 100;
            $('#ui-datepicker-div').css({'top': offsetLeft});
        }


    })


});



//
if($('#sw_feedback').length>0){
    $('#sw_feedback').validate({
        errorClass:'error-input',
        validClass:'succes-input',
        highlight: function (element, errorClass, validClass) {
            $(element).addClass(errorClass).removeClass(validClass);
        },
        unhighlight: function (element, errorClass, validClass) {
            $(element).removeClass(errorClass).addClass(validClass);
        },
        rules: {
            sw_fio: {
                required: true,
            },
            sw_rewies: {
                required: true
            },
        },
        submitHandler:function(form){
            var form = $(form),
                form_result = form.find('.form_check');
            $.ajax({
                url: form.attr('action'),
                type: form.attr('method'),
                async : true,
                data: form.serialize(),
                beforeSend: function(){
                    $('#loading').fadeIn('fast');
                },
                success: function(data){
                    if(data == 1){
                        form.trigger('reset');
                        $('#rew_form').fadeOut();
                        $('#thanks_rew').fadeIn(300);
                        //$('.form-shadow-call').removeClass('flex');
                        //$('html').removeClass('hidden-content');
                        //$('html').addClass('hidden-content');
                        //$('.form_send').addClass('flex').animate({'opacity':'1'},300);
                    }else{
                        form_result.html(data);
                    }
                    $('#loading').fadeOut('fast');
                },
                error: function(){
                    $('#loading').fadeOut('fast');
                }
            });
            return false;
        }
    });
}


