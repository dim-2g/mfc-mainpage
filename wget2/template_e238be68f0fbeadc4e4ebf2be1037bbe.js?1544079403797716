$(function(){
	$(document).on('click','.jsWhatForAgent__tab',function(e){
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var tabs = _this.parents('.jsWhatForAgent__tabs').eq(0);
		var tabClassAcive = tabs.attr('data-class-active');
		var boxes = $('.jsWhatForAgent__boxes',parent);
		var boxClassActive = boxes.attr('data-class-active');
		$('.jsWhatForAgent__tab').removeClass(tabClassAcive)
		_this.addClass(tabClassAcive);
		
		$('.jsWhatForAgent__box',parent).removeClass(boxClassActive);
		$('.jsWhatForAgent__box',parent).eq(_this.index()).addClass(boxClassActive);
		if($(window).width() < 1024) {
			var height = parent.offset().top - 70;
			$("body,html").animate({"scrollTop":height},100);
		}
		return false;
	});
	
	
	if($(".jsWhatForAgent__boxes").length){
		var jsWhatForAgentTimeout;
		$(window).on('load resize',function(){
			clearTimeout(jsWhatForAgentTimeout);
			jsWhatForAgentTimeout = setTimeout(function(){
				$(".jsWhatForAgent__boxes").each(function(){
					var parent = $(this);
					$(".jsWhatForAgent__box",parent).css('min-height',''); 
					var boxClassActive = parent.attr('data-class-active');
					var boxActiveWidth = $("."+boxClassActive).width();
					var maxHeight = 0;
					$(".jsWhatForAgent__box",parent).each(function(){
						var box = $(this);
						console.log(boxActiveWidth);
						box.css({'width':boxActiveWidth+'px','positin':'absolute','visibility':'hidden', 'display':'flex'});
						var curHeight = box.height();
						box.css({'width':'','position':'','visibility':'', 'display':''});
						
						
						if(maxHeight < curHeight){
							maxHeight = curHeight;
						}
					});
					$(".jsWhatForAgent__box").css('min-height',maxHeight+'px'); 
				})
			},250);
		});
	}
});
