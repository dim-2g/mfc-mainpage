$(function () {
    $('.footer-search').on('click touchstart', function (e) {
        e.stopImmediatePropagation();
        $(this).hide();
        $('.mobileSearch-input').focus();
        $('body').addClass('js-jivo_hide');
        $('.mobile-header-search').animate({
            bottom: 0,
            opacity: 1
        }, 200);
    })

    $('.mobileSearch-close').on('click touchstart', function(e){
        $('.mobile-header-search').animate({
            bottom: -54,
            opacity: 1
        }, 50);
        $('.footer-search').show();
        $('body').removeClass('js-jivo_hide');
        $('.mobileSearch-input').val('');
    })
});




