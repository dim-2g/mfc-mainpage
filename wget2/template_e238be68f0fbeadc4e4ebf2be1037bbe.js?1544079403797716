$(function () {
    $('.print').click(function (e) {
        e.stopImmediatePropagation();
        var imgPath = '/local/templates/ritual/images/MFPC_Ritual.ru.jpg';
        var html_to_print = '<img width="700" src="' + imgPath + '"/>';
        var iframe = $('<iframe id="print_frame">');
        $('body').append(iframe);
        var doc = $('#print_frame')[0].contentDocument || $('#print_frame')[0].contentWindow.document;
        var win = $('#print_frame')[0].contentWindow || $('#print_frame')[0];
        doc.getElementsByTagName('body')[0].innerHTML = html_to_print;

        var result = win.document.execCommand('print', false, null);

        if (!result)
            win.print();


        $('iframe').remove();
    });
});

function defineImgWidth() {

    if ($(window).width() <= 600) {

        $('.inner-text-content img').each(function () {
            if ($(this).width() > 500) {
                $(this).css({'width': '100%'});

            }
        })

    } else {
        $('.inner-text-content img').css({'width': ''});

    }
    //console.clear()


}
$(document).ready(function(){


    if ($('.news-title').length) {
        $('.news-title').each(function(){
            var text = $(this).text();
            if (text.length > 100) {
                var newText = $(this).text().slice(0,100);
                $(this).text(newText + '...');
            }
        })
    }


    $('input[name=phone],input[name=phoneFF], input[name=sw_phone_jou]').not(".mask-inited").inputmask({
        definitions: {
            '#': {
                validator: "[0-9]",
                cardinality: 1
            }
        },
        mask: "+7 (###) ### ## ##",
        placeholder: "_"
    }).addClass("mask-inited");

    $('.catted-button').click(function(e) {
        e.stopImmediatePropagation();
        $(this).parent('.catted-block').find('.catted-content').slideToggle(300);
        if ($(this).text() === '') {
            $(this).text('');
        } else {
            $(this).text('');
        }
    })


    $('h1').each(function(){
        if ($(this).text() === ' ') {
            $(this).hide();
        }
    })


    $('p').each(function(){
        if ($(this).text() === '') {
            $(this).hide();
        }
    })



    function defineCloseButton() {
        var footerSwitch = '<div class="footer-title-switch"></div>';
        $('.footer-title').append(footerSwitch);
    }

    function defineCloseButtonVisibility() {
/*         $('.menu-menu_link').each(function(){
            var text = $(this).text();
            $(this).html('<span>' + text + '</span>');
        }) */
    }


    function defineMobilMenuClasses() {

        if ($(window).width() < 1031) {
            $('.main-menu_list_item').each(function(){
                if ($(this).find('.submenu-inner').length) {
                    $(this).addClass('backgrounded');
                }
            })
        } else {
            $('.main-menu_list_item').removeClass("backgrounded");
        }


    }

    defineCloseButton();
    defineCloseButtonVisibility();
    defineMobilMenuClasses();
    defineImgWidth();



    $('.popup').find('h3').each(function(){
        var text = $(this).text();
        var newTitle = '<div class="form-title" style="font-size: 24px; color: #000000; font-family: "Arial";">' + text + '</div>';
        $(this).replaceWith(newTitle);

    });


    if ($('.zoom-link').length) {

        if ($(window).width() > 600) {
            $('.zoom-link').fancybox()
            $('.svg-wrap-catalog').on('click',   function(){
                $('.zoom-link').fancybox()
            })
        }  else {

            $('.zoom-link').find('img').appendTo($('.zoom-link').parent());


            $('.zoom-link').click(function(){
                return false;
            })

            $('.zoom-link img').on('click', function(e){
                e.preventDefault();
            })



        }


    }
    $('.top-red-text .top-red-text-item.pop').click(function(e){
        e.stopPropagation();
        e.preventDefault();
        return false;
    })

    $('.main-menu_list_item').on('mouseover', function(){
    });
    
    if ($(this).width() > 1031) {
		var timerOpenMenu;
        $('.main-menu_list_item').hover(function(){
			timerOpenMenu = setTimeout(function(_this){
				if (_this.find('.submenu-inner')) {
				   _this.find('.submenu-inner').stop().slideDown('150');
				}
			},150,$(this));

        }, function(){
			clearTimeout(timerOpenMenu);
            if ($(this).find('.submenu-inner')) {
                $(this).find('.submenu-inner').stop().slideUp('150');
            }
        })
    }



    $('.burger').click(function () {
        $('.burger').toggleClass('open-menu-switch');

        if ($('.main-menu').css('visibility') == 'hidden' &&  $(this).hasClass('open-menu-switch')) {
            $('.main-menu').stop().css({'visibility': 'visible'}).animate({'opacity': '1', 'left': '0'}, 800)
        }
        else if ($('.main-menu').css('visibility') == 'visible' && !($(this).hasClass('open-menu-switch'))) {
            $('.main-menu').stop().animate({'opacity': '0', 'left': '-200px'},800, function () {
                $('.main-menu').css({'visibility': "hidden"})
            });
        }
    });


    $('.question-title').click(function(e){
        e.stopImmediatePropagation();
        $(this).next('.question-content').slideToggle(300);
    })

    var htmlWidth = $('html').width();
    //$('.main-menu').css({'width':htmlWidth});
    //$('.fixed-header').css({'width':htmlWidth});
    //$('.header.inner-header').css({'width':htmlWidth});



    $(window).resize(function(){
        var htmlWidth = $('html').width();
		//$('.main-menu').css({'width':htmlWidth});
        //$('.fixed-header').css({'width':htmlWidth});
        //$('.header.inner-header').css({'width':htmlWidth});
        defineCloseButtonVisibility();
        defineMobilMenuClasses();

    })


    var hrefL = location.href;

    $('.menu-menu_link').each(function(e){


        if ($(this).text() == ' ') {
            $(this).attr('href', '/catalog/');
        }

    })
    $('.transFive.rosgosstrah').click(function(){
        location.href = "/ritualnie-uslugi/ritualnoe-strakhovanie/";
    })
    if ($(window).width() < 1031) {
        $('.main-menu_list_item').click(function(e){


            e.stopImmediatePropagation();
            var target = e.target;

            if (target.tagName !== 'SPAN'  && $(this).hasClass('backgrounded')) {
                e.preventDefault();

                if ($(this).hasClass('switched-background')) {
                    $('.submenu-inner').hide();
                    $(this).removeClass('switched-background');
                }  else {
                    $('.submenu-inner').hide();

                    $(this).toggleClass('switched-background');
                    $(this).find('.submenu-inner').slideToggle(300);



                }

            }


        })


        $('.footer-title').click(function(e){
            e.stopImmediatePropagation();
            if ($(this).hasClass('switched-title')) {
                $('.footer-col-blocks').hide();
                $(this).removeClass('switched-title');
            }       else {

                $('.footer-col-blocks').hide();
                $('.footer-title').removeClass('switched-title');
                $(this).addClass('switched-title');
                $(this).next().slideToggle(300);
            }
        })


    }




    //console.clear()


    if ($('.product-slider-wrap').length) {

        $('.product-slider-wrap').slick({
            infinite: true,
            slidesToShow: 3,
            slidesToScroll: 3,
            speed: 500,

            arrows: false,
            responsive: [
                {
                    breakpoint: 1100,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                },
                {
                    breakpoint: 800,
                    settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1,



                    }
                }
            ]
        });

        $('.arrow-left').click(function () {
            $('.product-slider-wrap').slick('slickPrev');

        })

        $('.arrow-right').click(function () {
            $('.product-slider-wrap').slick('slickNext');
        })
    }



    
    if ($('.services-cols').length) {
        $('.services-cols').slick({
            infinite: true,
            slidesToShow: 3,
            slidesToScroll: 3,
            speed: 500,
            prevArrow: '<div class="service-arrow-left"></div>',
            nextArrow: '<div class="service-arrow-right"></div>',
            responsive: [
                {
                    breakpoint: 1130,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                },
                {
                    breakpoint: 700,
                    settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                }
            ]
        });
    }



    var width = $(window).width();
    if(width < 810) {
        $('br').each(function(){
            //$(this).css("display", "none");
        });
    }
	
    $('.pop').click(function(e){
        $('#order .form-title').text(' ');
        var title = $(this).data('title');
        $('html').addClass('fancybox-lock');
        $('html').addClass('fancybox-margin');
        var htmlWidth = $('html').width();
        $('.main-menu').css({'width':htmlWidth});
        $('.fixed-header').css({'width':htmlWidth});
        $('input[name=sub]').val(title);
        $('.overlay, #order').fadeIn(300);
        $('#order h3').text(title);
        $('#order .form-title').text(title);

        // reset form
        $('form').trigger("reset");
    });
    $('.agent_rewiev_button').click(function(e){
        var title = $(this).data('title'),
        	name_agent = $(this).data('name');
        $('html').addClass('fancybox-lock');
        $('html').addClass('fancybox-margin');
        var htmlWidth = $('html').width();
        $('.main-menu').css({'width':htmlWidth});
        $('.fixed-header').css({'width':htmlWidth});
        $('input[name=sub]').val(title);
        $('.overlay, #rewiev_popup').fadeIn(300);
        $('#rewiev_popup h3').text(title);
        $('#rewiev_popup .form-title').text(title);
        $('#name_agent').val(name_agent);

        // reset form
        $('form').trigger("reset");
    });
    $('.header_left a').click(function(){

        $('html').addClass('fancybox-lock');
        $('html').addClass('fancybox-margin');
        $('.codesc_popup,.overlay').fadeIn(300);
        return false;
    });
    
    $(document).on('submit', '#order', function(e) {
        e.preventDefault();
        var yacity = $('#f-city').val();
        var f = $(this);
        $('input[type=text]', f).removeClass('ierror');

        var name = $('.popup_form input[name=name]').val();
        var phone = $('.popup_form input[name=phone]').val();
        var agree = $('.popup_form input[name=agree]:checked').val();
        var recaptcha = $('textarea[name=g-recaptcha-response]').val();

        var query = 'act=sender';
        query += '&name=' + name;
        query += '&phone=' + phone;
        query += '&agree=' + agree;
        query += '&g-recaptcha-response=' + recaptcha;
        query += '&sub=' + $('#order .form-title').text();


        $.ajax({
            type: "POST",
            data: query,
            url: "/local/functions/sender.php",
            dataType: "json",
            success: function(data) {
              if(data.result == 'ok') {
                $('.overlay, #order').fadeOut();
                $("#thanks_request").show();
              } else {
                $('#order .error').text(data.text);
              }
            }
        }); 
        //$(this).trigger("reset");
        grecaptcha.reset();
        return false;
    });
    
    $(document).on('click', '#rewiev_popup input[type="submit"]', function(e) {
        e.preventDefault();
        var f = $(this);
        $('input[type=text]', f).removeClass('ierror');

        var name = $('.popup_form input[name=name]').val();
        var agent_name = $('.popup_form input[name=name_agent]').val();
        var message = $('.popup_form textarea[name=message]').val();
        var recaptcha = $('textarea[name=g-recaptcha-response]').val();
	   	if(!name){
	   		$('#rewiev_popup .error').text(",  ");
	   		return;
	   	}
	   	if(!message){
	   		$('#rewiev_popup .error').text(",  ");
	   		return;
	   	}
        var query = 'act=rewiev';
        query += '&name=' + name;
        query += '&agent_name=' + agent_name;
        query += '&message=' + message;
        query += '&g-recaptcha-response=' + recaptcha;

        $.ajax({
            type: "POST",
            data: query,
            url: "/local/functions/sender_rewiev.php",
            dataType: "json",
            success: function(data) {
                if(data.result == 'ok') {
                    $('#rewiev_popup').fadeOut();
                    $("#thanks_review").show();
                } else {
                    $('#rewiev_popup .error').text(data.message);
                }
            }
        });

        $(this).trigger("reset");
        grecaptcha.reset();
        return false;
    });
    $('.overlay,.closed').click(function(){

        $('.overlay,.popup_form').fadeOut(300);
        setTimeout(function(){
            $('html').removeClass('fancybox-lock');
            $('html').removeClass('fancybox-margin');
        },300);

    });
    $('.fancybox2').click(function(){
        var top = $(this).offset().top;

        $('.popup_text').css('top',top+'px');
        $('html,body').animate({scrollTop:top-60+'px'},300);
        $('.overlay,.popup_text').fadeIn(300);
    });


    $('.buttons button').click(function(){
		var _this = $(this);
		if(_this.hasClass('active')) return false;
        var tab = _this.attr('data-tab');
        $('.tab_content').hide();
        $(tab).fadeIn(300);
        if(tab == '#tab1') {
            $('.tab1').show();
        } else {
            $('.tab1').hide();
        }
        $('.buttons button').removeClass('active');
        _this.addClass('active');
    });

    var owl = $("#spisok")
    owl.owlCarousel({
        loop:true,
        nav:true,
        dots:false,
        responsive : {
            // breakpoint from 0 up
            0 : {
                items:1
            },
            800 : {
                items:1
            },
            1260 : {
                items:3
            }
        }
    });
    $('.ras').owlCarousel({
        loop:true,
        nav:true,
        dots:false,
        items:1
    });

    owl.on('changed.owl.carousel', function(event) {
        $("#spisok .item").css('opacity','1');
        var item      = event.item.index;
        $("#spisok .item").eq(item).css('opacity','0.4');
        $("#spisok .item").eq(item+2).css('opacity','0.4');
    });
    $('.tab_content .items .item label').click(function(){

        if($(this).attr('data-img')) {
            $('html').addClass('fancybox-lock');
            $('html').addClass('fancybox-margin');
            var img = $(this).attr('data-img');
            $(this).find('input').attr('checked','checked');
            $('.popup img').attr('src','');
            $('.popup img').attr('src',img);
            $('.popup').fadeIn(300);
        }
        else {


        }
        return false;
    });
    $('.popup span,.popup').click(function(){
        $('.popup').fadeOut(300);
        setTimeout(function(){
            $('html').removeClass('fancybox-lock');
            $('html').removeClass('fancybox-margin');
        },300);
    });

    new WOW().init();

});


$(document).ready(function(){


    $('.coffin-input').click(function (e) {
        e.stopImmediatePropagation(e);
        $(this).removeClass('error-input');
        $('.coffin-input-block').not($(this).parent()).find('.coffin-list').hide();
        $(this).parent().find('.coffin-list').slideToggle(300);
        $(this).toggleClass('deployed');
    });


    $('.follow-link.pop').click(function(){
        $('.form-title').text('  ')
    })


    $('.img-thumbnail').click(function(){
        $('.img-thumbnail').removeClass('active-thumb');
        $(this).addClass('active-thumb');
        var imgBigSrc = $(this).attr('data-src');
        $('.img-big').attr('src', imgBigSrc);
    });


    function slide(dir) {
        var current;
        var imgBigSrc;
        $('.img-thumbnail').each(function(){
            if ($(this).hasClass('active-thumb')) {
                current = $(this).index();
            }
        })





        if (dir === 'left') {

            if (current == 0) {
                return;
            }
            $('.img-thumbnail').removeClass('active-thumb');
            $('.img-thumbnail').eq(current).prev().addClass('active-thumb');
            imgBigSrc = $('.img-thumbnail').eq(current).prev().attr('data-src');


        }  else {

            if (current == $('.img-thumbnail').last().index()) {
                return;
            }
            $('.img-thumbnail').removeClass('active-thumb');
            $('.img-thumbnail').eq(current).next().addClass('active-thumb');
            imgBigSrc = $('.img-thumbnail').eq(current).prev().attr('data-src');

        }


        $('.img-big').attr('src', imgBigSrc);
    }


    $('.arrow-left').click(function(){
        slide('left');

    })


    $('.arrow-right').click(function(){
        slide('right');
    })
})



function defimeMenuWidth() {
    var width = $('.inner-text-content').outerWidth();
    var left = $('.inner-text-content').offset().left;
    $('.addition-menu').css({'width':width, 'left':left});
}

$(document).ready(function() {
//c
    var owl = $('.ritual-slider-wrap .owl-carousel');
    owl.owlCarousel({
        margin: 10,
        nav: true,
        loop: false,
        responsive: {
            0: {
                items: 1
            },
            600: {
                items: 3
            },
            1000: {
                items: 4
            }
        }
    });
    $('.owl-prev, .owl-next').text('');

// 
    if ($('.inner-text-content').length) {
        defimeMenuWidth();
        $(window).resize(function(){
            defimeMenuWidth();
            defineImgWidth();
            defineImgBorders();
        });
    }

    $('.service-tab').click(function(){
        $('.service-tab').removeClass('active-tab');
        $(this).addClass('active-tab');
        $('.services-content').removeClass('active-content');
        $('.services-content').eq($(this).index()).addClass('active-content');
    })


    $('.catalog-filter .catalog-title').click(function (e) {
        e.stopImmediatePropagation();

        var target = e.target;
        if (!$(target).hasClass('catalog-title-link-follow')) {


            $(this).toggleClass('active');
            $(this).next().slideToggle(300);
        }

    });


/*     $('body').on('click', '.catalog-good', function(){
        var href = $(this).find('a').attr('href');
        location.href = href;
    }) */



    if ($(window).width() < 600) {
        $('.catalog-title').removeClass('active');
    }



    if ($('.filter-sel-title').length) {





        $('.filter-sel-title').sun({
            wrapperClass: 'reason-select',
            selClass: 'sel',
            optClass: 'option',
            arrowClass: 'arrow',
            maxListHeight: 200
        });


        $('.catalog-title-link').click(function () {
            return false;
        })
    }

    function defineImgBorders() {
        var $filtered;

        if ($(window).width() > 1330) {


            $filtered = $('.catalog-good').filter(function(index){
                return index > $('.catalog-good').length - 4
            });


        }


        if ($(window).width() < 1330 && $(window).width() > 930) {


            var $filtered = $('.catalog-good').filter(function(index){
                return index > $('.catalog-good').length - 3
            });


        }


        if ($(window).width() < 930) {


            var $filtered = $('.catalog-good').filter(function(index){
                return index > $('.catalog-good').length - 2
            });

        }


        if ($filtered !== undefined) {
            $filtered.css({'border-bottom':'none'});
        }



    }


    defineImgBorders();






    $('.product-slider-item').click(function(){
        var href = $(this).find('a').attr('href');
        location.href = href;
    })



    if ($(window).width() > 1031) {
        for (var i = 1; i < $('.product-slider-item').length; i += 3) {
            $('.product-slider-item').eq(i).addClass('centered-item');
        }

        if ($('.product-slider-item').length % 3 == 1) {
            $('.product-slider-item').eq($('.product-slider-item').length - 4).addClass('centered-item');
        }
    }



    $('.submenu-inner .submenu-list').each(function(){
        if ($(this).children().length > 6) {
            $(this).parent().addClass('submenu-inner_large');
        }
    })


})

$(function () {


    $('.calc-btn').on('mousedown touchstart', function () {
        $('.calc-btn').removeClass('active');
        $(this).addClass('active');
        $('.caluc-wrap').removeClass('active-wrap');
        $('.caluc-wrap').eq($(this).index()).addClass('active-wrap')
    });


    $('.coffin-input').click(function () {
        $(this).removeClass('error-input');
        $('.coffin-input-block').not($(this).parent()).find('.coffin-list').hide();
        $(this).parent().find('.coffin-list').slideToggle(300);
        $(this).toggleClass('deployed');
    });

    function addSpace(elem) {
        return elem.replace(/(.)(?=(.{3})+$)/g, "$1 ");
    }

    function digits(elem) {
        elem.textContent = addSpace(elem.textContent) + ' ' + '.'
    }

    $('body').click(function (e) {
        if (!$(e.target).hasClass('coffin-input')) {
            $('.coffin-list').hide();
            $('.coffin-input').removeClass('deployed');
        }

    });


    function showButton(elem, e) {
        e.stopImmediatePropagation();
        var inputGroups = elem.children().not('.cof-class');
        var lenght = inputGroups.length;
        inputGroups.find('.button-calc').show();
        inputGroups.find('.plus, .minus').show();

        if (lenght < 1) {
            return
        }
        else if (lenght == 1) {
            inputGroups.first().children().eq(3).show()
            // inputGroups.first().find('.button-calc').show();
            inputGroups.find('.plus').show();
            inputGroups.find('.minus').hide();
        }
        else if (lenght == 2) {
            inputGroups.first().find('.buttons-calc').hide();
            inputGroups.eq(1)
                .find('.plus').show().end()
                .find('minus').show();

        }
        else if (lenght > 2) {
            inputGroups.find('.button-calc').show();
            inputGroups.find('.plus, .minus').show();
            inputGroups.first().find('.buttons-calc').hide();
            inputGroups.each(function (i) {
                if (i > 0 && i < lenght - 1) {

                    $(this).find('.plus').hide();
                    $(this).find('.minus').show()
                }
            });
            inputGroups.last()
                .find('.plus, .minus').show()

        }
        if (elem.hasClass('coffin-grob') && lenght === 3) {

            inputGroups.eq(1).children().eq(3).show()
            // inputGroups.first().find('.button-calc').show();
            inputGroups.find('.plus').show();
            inputGroups.find('.minus').hide();
        }

    }

    rubles = 0;

    $('body').on('click', '.coffin-list-item', function () {
        if ($('.caluc-wrap').hasClass('active-wrap')) {
            rubles = 0;
            var greatParent = $(this).parent().parent();
            $('.coffin-input').removeClass('deployed');
            greatParent.find('.coffin-price').text('');
            $(this).parent().slideUp(300);
            greatParent.find('.coffin-price').attr('data-price', $(this).attr('data-price'));
            greatParent.find('.coffin-price').text($(this).attr('data-price'));
            digits(this.parentNode.parentNode.querySelector('.coffin-price'));
            greatParent.find('.coffin-input').text($(this).text());

            if(!$(this).hasClass('noSelectCoffin')) {
                greatParent.find('.coffin-input-title').addClass('active');
                greatParent.find('.coffin-input').addClass('active');
            }
            else{
                greatParent.find('.coffin-input-title').removeClass('active');
                greatParent.find('.coffin-input').removeClass('active');
            }


            calculateSumm();
            $('.active-wrap').find('.hidden-form').html('');
            createForm();

        }

    });


    $('body').on('click', '.plus', function (e) {
        if ($('.caluc-wrap').hasClass('active-wrap')) {
            $('.coffin-input').removeClass('deployed');
            $('.coffin-list').hide();
            var clone = $(this).parent().parent().clone(true);
            clone.find('.coffin-price').text('0 .').attr('data-price', 0).hide();
            clone.find('.coffin-input-title').hide();
            clone.find('.coffin-input').removeClass('active').text(' ');

            $(this).parent().parent().parent().append(clone);
            $('.active-wrap').find('.hidden-form').html('');

            createForm();
            showButton($(this).parent().parent().parent(), e)
        }
    });
    $('body').on('click', '.minus', function (e) {
        if($('.caluc-wrap').hasClass('active-wrap')) {
            $('.coffin-input').removeClass('deployed');
            $('.coffin-list').hide();
            var father = $(this).parent().parent();
            var grandFather = father.parent();
            var prev = father.prev();
            $(this).parent().parent().remove();
            showButton(grandFather, e);
            rubles = 0;
            calculateSumm();
            $('.active-wrap').find('.hidden-form').html('');
            createForm();
        }

    });
    var rubles = 0;

    function calculateSumm() {
		var rubles = 0;
		var _active =  $('.active-wrap');
        _active.find('.coffin-price').each(function () {
			var _this = $(this);
            if (_this.attr('data-price')) {
                rubles += parseInt(_this.attr('data-price'));
            }
        });
		var calc = $('.jsCalculator',_active);
		if(calc.length){
			var discountSwitch = $(".jsCalculator__discount-switch",calc);
			if(discountSwitch.is(':checked')){
				discount = parseInt(calc.eq(0).attr('data-discount')) || 0;
				if(rubles >= discount){
					rubles -= discount;
				} else {
					rubles = 0;
				}
			}
			$('.active-wrap .allSumm').text(rubles);
			digits(document.querySelector('.active-wrap .allSumm'))		
		}
		
        $('.active-wrap').find('.calc-clone').each(function (i) {
            var allSumm = 0;
            $(this).find('.coffin-price').each(function (i, item) {
                var price = parseInt($(this).attr('data-price'));
                allSumm += price;


            })

            $(this).find('.coffin-price').eq(0).text(allSumm);
            digits(this.querySelector('.coffin-price'))

        })

    }
	
	$(document).on('change','.jsCalculator__discount-switch',function(e){
		calculateSumm();
		$('.active-wrap').find('.hidden-form').html('');
		createForm();
	});	


    calculateSumm();

    function createForm() {
        $('.active-wrap').find('.coffin-input-title').each(function () {
            var fieldset = document.createElement('fieldset');
            fieldset.className = 'hiddenFieldset';
            var wordInput = document.createElement('input');
            wordInput.setAttribute('name', $(this).attr('data-name') + '[]');
            var name;
            if ($(this).find('span').first().hasClass('coffin-price') || $(this).find('span').first().hasClass('requir') || $(this).find('span').first().hasClass('notitle')) {
                name = $(this).parent().parent().prev().text();
            } else
                name = $(this).find('span').first().text();
            var price = $(this).find('.coffin-price').text();
            var text = $(this).next().text();
            wordInput.value = name + ":" + price + ":" + text;
            fieldset.appendChild(wordInput);
            document.querySelector('.active-wrap .hidden-form').appendChild(fieldset)
        });
		
		var _active = $('.active-wrap');
		if(_active){
			var calc = $('.jsCalculator',_active);
			if(calc.length){
				var discountSwitch = $(".jsCalculator__discount-switch",calc);
				if(discountSwitch.length){
					if(discountSwitch.is(':checked')) {
						$(".hidden-form",_active).append('<input type="hidden" name="allowance" value="Y" />');
					}
				}
			}
		}
    }

    createForm();



    var error;

    $('.issue').on('click touchstart', function () {


        $('.active-wrap .required-coffin').each(function () {
            if (!$(this).hasClass('active')) {
                error = true;
                $(this).addClass('error-input');
            }
            else {
                error = false;
            }
        })
        if (error) {
            var firstOffset = $('.active-wrap .required-coffin.error-input').first().offset().top - $('.header-top-wrap').outerHeight() - 150;
            $('body:not(":animated"), html:not(":animated")').animate({
                scrollTop: firstOffset
            }, 500);
        }
        if (!error) {
            $('.active-wrap .form-vac').fadeIn(300).toggleClass('active');
            $('html').addClass('ovHid');
            $('html').addClass('fancybox-lock');
            $('html').addClass('fancybox-margin');
        }

    });

});

