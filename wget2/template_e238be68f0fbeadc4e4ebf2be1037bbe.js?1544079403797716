$(function () {
	var page = $('.jsPage');

	page.on('click', '.jsPage__mobile-menu-show', function (e) {
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var body = $('body');
		$('.jsPage__menu', parent).addClass('switched-menu');		
		body.addClass(body.attr('data-class-hide-jivo'));
		return false;
	});

	page.on('click', '.jsPage__mobile-menu-close', function (e) {
		e.preventDefault();
		var _this = $(this);
		var parent = _this.parents(e.handleObj.selector.split('__')[0]).eq(0);
		var body = $('body');
		$('.jsPage__menu', parent).removeClass('switched-menu');
		body.removeClass(body.attr('data-class-hide-jivo'));
		return false;
	});

	var page__content = $('.jsPage__content');
	var page__footer = $('.page__footer');
	var page__leftSide = $('.jsPage__left-side');
	var page__leftSideCont = $('.jsPage__left-side-cont');
	var jsPageleftSideFixed = false;
	$(window).on('load scroll', function () {
		if (page__leftSide.length) {
			var scrollTop = $(window).scrollTop();
			var pageHeight = $(window).height();
			var footerOffset = page__footer.offset().top;
			var sideOffset = page__leftSideCont.offset().top;
			var sideHeight = page__leftSideCont.height();
			var bottomleftSide = sideOffset - scrollTop + sideHeight + 3;

			if (jsPageleftSideFixed) {
				bottomleftSide = lastBottomleftSide;
			}
			if (footerOffset - scrollTop > bottomleftSide) {
				page__leftSide.css('bottom', '');
				jsPageRightSideFixed = false;
			} else {
				if (window.innerWidth < 1382 && window.innerWidth > 1024) {
					page__leftSide.css('bottom', scrollTop + pageHeight - footerOffset + sideHeight / 2 + 4 + 'px');
					bottomleftSide - 3;
				} else if (window.innerWidth <= 1024 && window.innerWidth > 768) {
					page__leftSide.css('bottom', scrollTop + pageHeight - footerOffset + sideHeight - 116 + 1 + 'px');
					bottomleftSide - 3;
				} else if (window.innerWidth <= 768 ) {
					page__leftSide.css('bottom', scrollTop + pageHeight - footerOffset + sideHeight - 113 + 1 + 'px');
					bottomleftSide - 3;
				} else {
					page__leftSide.css('bottom', scrollTop + pageHeight - footerOffset + sideHeight / 3 - 12 + 'px');
				}
				lastBottomleftSide = bottomleftSide;
				jsPageleftSideFixed = true;
			}
		}
	});
});


$(function () {
	var page__rightSide = $('.jsPage__right-side');
	var page__rightSideCont = $('.jsPage__right-side-cont');
	var page__content = $('.jsPage__content');
	var page__footer = $('.page__footer');
	var jsPageRightSideFixed = false;

	$(window).on('load scroll', function () {
		if (page__rightSide.length) {
			var scrollTop = $(window).scrollTop();
			var pageHeight = $(window).height();
			var footerOffset = page__footer.offset().top;
			var sideOffset = page__rightSideCont.offset().top;
			var sideHeight = page__rightSideCont.height();
			var bottomRightSide = sideOffset - scrollTop + sideHeight;

			if (jsPageRightSideFixed) {
				bottomRightSide = lastBottomRightSide;
			}
			if (footerOffset - scrollTop > bottomRightSide) {
				page__rightSide.css('bottom', '');
				jsPageRightSideFixed = false;
			} else {
				page__rightSide.css('bottom', scrollTop + pageHeight - footerOffset + sideHeight / 2 + 1 + 'px');
				lastBottomRightSide = bottomRightSide;
				jsPageRightSideFixed = true;
			}
		}
	});
});
/* End */
